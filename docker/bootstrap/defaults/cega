#!/usr/bin/env bash
set -e

OPENSSL=openssl
LEGA_INSTANCES="swe1,fin1"

declare -A CEGA_MQ
CEGA_MQ['swe1']=$(generate_password 16)
CEGA_MQ['fin1']=$(generate_password 16)

